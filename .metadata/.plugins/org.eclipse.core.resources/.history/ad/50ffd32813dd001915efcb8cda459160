package project4;

import java.util.Scanner;

public class Synthesize_Practise1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//»Æ½ð·Ö¸îµã
		float ans;
		float temp = 1;
		int k=0,p=0;
		for(int i = 1;i<=20;i++){
			for(int j = 1;j<=20;j++){
				if(i%2==0&&j%2==0)
				{
					continue;
				}
				ans = (float)(i/j);
				if((ans-0.618)<(temp-0.618))
				{
					temp = ans;
					k = i;
					p = j;
				}
			}
		}
		System.out.println(k+""+p);
	}

}
